{
  "name": "db-main-kysely",
  "version": "1.0.0",
  "private": true,
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "scripts": {
    "migrate": "tsx -r dotenv/config scripts/migrator.ts",
    "codegen": "kysely-codegen --camel-case --dialect postgres --out-file src/generated/db.ts",
    "build": "rm -fr dist && tsc -d",
    "dev": "tsx -r dotenv/config watch --inspect src/index.ts",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "kysely": "^0.26.3",
    "pg": "^8.11.3"
  },
  "devDependencies": {
    "@types/node": "^18.18.6",
    "@types/pg": "^8.10.7",
    "dotenv": "^16.3.1",
    "eslint-config-custom": "workspace:^",
    "kysely-codegen": "^0.11.0",
    "tsconfig": "workspace:^",
    "tsx": "^3.0.0",
    "typescript": "^5.2.2"
  }
}
